    "migrate-reset": "keystone prisma migrate reset"
  },
  "dependencies": {
    "@keystone-6/auth": "5.0.1",
    "@keystone-6/cloudinary": "5.0.1",
    "@keystone-6/core": "3.1.3",
    "@keystone-6/fields-document": "5.0.2",
    "next": "^12.0.0",
    "stripe": "^11.0.0",
    "typescript": "^4.8.0"
  }
}
